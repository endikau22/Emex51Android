<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reto2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/area51"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/area51"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".CalendarioActivity"></activity>
        <activity android:name=".ActivityListaSectores" />
        <activity android:name=".ActivitySectores" />
        <activity android:name=".ActivityEditPassword" />
        <activity android:name=".ActivityMiCuenta" />
        <activity android:name=".ActivityPortada" />
        <activity android:name=".RecuperarActivity" />
        <activity android:name=".ActivityInicioSesionRegistro" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>